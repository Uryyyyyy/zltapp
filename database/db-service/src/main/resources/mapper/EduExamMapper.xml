<mapper namespace="com.zlt.mapper.EduExamMapper">

    <select id="findAllExam" resultType="com.zlt.pojo.EduExam">
        select * from edu_exam
    </select>

    <select id="findByExamId" resultType="com.zlt.pojo.EduExam" parameterType="java.lang.String">
        select * from edu_exam where examId=#{examId}
